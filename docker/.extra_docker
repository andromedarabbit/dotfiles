#!/usr/bin/env bash

alias dockerclean='docker rm $(docker ps -a -q); docker rmi $(docker images -a | grep "^<none>" | awk "{print $3}")'
alias dockercleanall='docker rm $(docker ps -a -q); docker rmi $(docker images -a | awk "{print $3}")'
alias dockerlogin='aws --region=us-east-1 ecr get-login | pbcopy && echo "Command is pasted into the clipboard!"'
alias dockerstop='docker stop $(docker ps -a -q)'
alias dockerrm='docker rm $(docker ps -a -q)'
